<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="swipeselector_color_indicator_active">#1E88E5</color>
    <color name="swipeselector_color_indicator_inactive">#DDDDDD</color>
    <string name="app_name">SwipeSelector</string>
    <string name="move_left">Move left</string>
    <string name="move_right">Move right</string>
    <declare-styleable name="SwipeSelector">
        <attr format="reference" name="swipe_itemsXmlResource"/>
        <attr format="string" name="swipe_unselectedItemTitle"/>
        <attr format="string" name="swipe_unselectedItemDescription"/>
        <attr format="dimension" name="swipe_indicatorSize"/>
        <attr format="dimension" name="swipe_indicatorMargin"/>
        <attr format="color" name="swipe_indicatorActiveColor"/>
        <attr format="color" name="swipe_indicatorInActiveColor"/>
        <attr format="reference" name="swipe_leftButtonResource"/>
        <attr format="reference" name="swipe_rightButtonResource"/>
        <attr format="string" name="swipe_customFontPath"/>
        <attr format="reference" name="swipe_titleTextAppearance"/>
        <attr format="reference" name="swipe_descriptionTextAppearance"/>

        <attr format="enum" name="swipe_descriptionGravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>
</resources>